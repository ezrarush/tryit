<h1>Listing menu</h1>
 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="3"></th>
  </tr>
 
  <% @menus.each do |menu| %>
    <tr>
      <td><%= menu.title %></td>
      <td><%= menu.text %></td>
      <td><%= link_to 'Show', menu_path(menu) %></td>
      <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
      <td><%= link_to 'Destroy', menu_path(menu), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New menu', new_menu_path %>

